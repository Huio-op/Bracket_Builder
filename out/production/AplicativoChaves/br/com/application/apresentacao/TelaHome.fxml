<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>

<AnchorPane fx:id="rootAnchorPane" prefHeight="750.0" prefWidth="950.0" stylesheets="@application.css" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.application.apresentacao.HomeController">
   <children>
      <StackPane fx:id="rootStackPane" prefHeight="750.0" prefWidth="950.0">
         <children>
            <AnchorPane fx:id="paneAnchor" prefHeight="750.0" prefWidth="950.0">
               <children>
                  <AnchorPane fx:id="anchorMain" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="700.0" prefWidth="950.0" style="-fx-background-color: #292E42;" />
                  <AnchorPane fx:id="anchorFooter" layoutY="700.0" maxHeight="-Infinity" maxWidth="-Infinity" prefHeight="50.0" prefWidth="950.0" style="-fx-background-color: #191D1F;">
                     <children>
                        <JFXButton fx:id="btnNewEvent" layoutX="745.0" layoutY="9.0" onAction="#createBracketTransition" prefHeight="31.0" prefWidth="190.0" styleClass="light-button" text="Novo Evento" />
                        <JFXButton fx:id="btnSeeEvents" layoutX="530.0" layoutY="9.0" onAction="#seeEventTransition" prefHeight="31.0" prefWidth="190.0" styleClass="dark-button" text="Ver Eventos Criados" />
                        <JFXButton layoutX="14.0" layoutY="9.0" onAction="#doLogOut" prefHeight="31.0" prefWidth="190.0" styleClass="red-button" text="Log Out" />
                     </children>
                  </AnchorPane>
               </children>
            </AnchorPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
