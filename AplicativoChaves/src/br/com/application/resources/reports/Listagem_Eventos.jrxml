	<queryString language="SQL">
		<![CDATA[SELECT e.id_evento,
       e.nome,
       j.nome AS nomeJogo,
       e.detalhes,
       e.premio,
       e.data,
       ou.nome AS nomeOrg
FROM evento e
         LEFT JOIN (SELECT o.cpf,
                           u.nome
                    FROM organizador o, usuario u WHERE
                            o.email = u.email
) ou ON e.cpf_organizador = ou.cpf, jogo j WHERE j.id_jogo = e.jogo
ORDER BY e.id_evento;]]>
	</queryString>
